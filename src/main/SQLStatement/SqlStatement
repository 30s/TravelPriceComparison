/*
putDataToHBaseDefault
*/

0.create 'TRAVEL','INFO'(HBase)

1.CREATE VIEW TRAVEL (ROWKEY VARCHAR PRIMARY KEY,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR)

2.CREATE TABLE TRAVELS (ROWKEY VARCHAR PRIMARY KEY,PAGEID BIGINT,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR)

3.CREATE SEQUENCE TRAVELPAGEID;

4.UPSERT INTO TRAVELS SELECT ROWKEY,NEXT VALUE FOR PRICEPAGEID,URL,SP,EP,ST,ET,SIGHTS,ALLDATE,HOTEL,TOTALPRICE,TRAFFIC,TRAVELTYPE,SUPPLIER,IMAGE FROM TRAVEL;


/*
putDataHBaseSortPrice
*/

0.create 'TRAVELSORTPRICE','INFO'(HBase)

1.CREATE VIEW TRAVELSORTPRICE (ROWKEY VARCHAR PRIMARY KEY,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR)

2.CREATE TABLE TRAVELSORTPRICES (ROWKEY VARCHAR PRIMARY KEY,PAGEID BIGINT,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR)

3.CREATE SEQUENCE PRICEPAGEID;

4.UPSERT INTO TRAVELSORTPRICES SELECT ROWKEY,NEXT VALUE FOR PRICEPAGEID,URL,SP,EP,ST,ET,SIGHTS,ALLDATE,HOTEL,TOTALPRICE,TRAFFIC,TRAVELTYPE,SUPPLIER,IMAGE FROM TRAVELSORTPRICE;


/*
putDataHBaseSortHotel
*/
0.create 'TRAVELSORTHOTEL','INFO'(HBase)

1.CREATE VIEW TRAVELSORTHOTEL (ROWKEY VARCHAR PRIMARY KEY,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR,INFO.HOTELLEVEL UNSIGNED_INT)

2.CREATE TABLE TRAVELSORTHOTELS (ROWKEY VARCHAR PRIMARY KEY,PAGEID BIGINT,INFO.URL VARCHAR,INFO.SP VARCHAR,INFO.EP VARCHAR,INFO.ST VARCHAR,INFO.ET VARCHAR,INFO.SIGHTS VARCHAR,INFO.ALLDATE VARCHAR,INFO.HOTEL VARCHAR,INFO.TOTALPRICE UNSIGNED_INT,INFO.TRAFFIC VARCHAR,INFO.TRAVELTYPE VARCHAR,INFO.SUPPLIER VARCHAR,INFO.IMAGE VARCHAR,INFO.HOTELLEVEL UNSIGNED_INT)

3.CREATE SEQUENCE HOTLEPAGEID;

4.UPSERT INTO TRAVELSORTHOTELS SELECT ROWKEY,NEXT VALUE FOR HOTLEPAGEID,URL,SP,EP,ST,ET,SIGHTS,ALLDATE,HOTEL,TOTALPRICE,TRAFFIC,TRAVELTYPE,SUPPLIER,IMAGE,HOTELLEVEL FROM TRAVELSORTHOTEL;

